{
  "name": "@assetpipe/core",
  "version": "0.1.0",
  "author": "",
  "license": "Apache-2.0",
  "description": "",
  "exports": {
    "./types": {
      "import": {
        "types": "./dist/types.d.mts",
        "default": "./dist/types.mjs"
      },
      "require": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      }
    },
    "./pipelines": {
      "import": {
        "types": "./dist/pipelines/index.d.mts",
        "default": "./dist/pipelines/index.mjs"
      },
      "require": {
        "types": "./dist/pipelines/index.d.ts",
        "default": "./dist/pipelines/index.js"
      }
    },
    "./runtime": {
      "import": {
        "types": "./dist/runtime/index.d.mts",
        "default": "./dist/runtime/index.mjs"
      },
      "require": {
        "types": "./dist/runtime/index.d.ts",
        "default": "./dist/runtime/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "dependencies": {
    "@parcel/watcher": "^2.5.1",
    "@types/picomatch": "^4.0.2",
    "jiti": "^2.6.1",
    "oxc-parser": "^0.107.0",
    "oxc-resolver": "^11.16.2",
    "picomatch": "^4.0.3"
  },
  "devDependencies": {
    "tsdown": "^0.15.6"
  },
  "nx": {
    "name": "core",
    "targets": {
      "build": {
        "inputs": [
          "{projectRoot}/**/*.ts"
        ],
        "outputs": [
          "{projectRoot}/dist"
        ]
      },
      "dev": {
        "continuous": true
      }
    }
  }
}
